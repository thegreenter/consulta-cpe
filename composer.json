{
    "name": "greenter/consulta-cpe",
    "description": "PHP Client de Consulta Integrada de Comprobante de Pago - SUNAT.",
    "keywords": [
        "sunat",
        "facturacion-electronica",
        "greenter"
    ],
    "homepage": "https://github.com/thegreenter/consulta-cpe",
    "license": "MIT",
    "authors": [
        {
            "name": "Greenter",
            "homepage": "https://greenter.dev"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2 || ^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "phpstan/phpstan": "^0.12.51"
    },
    "autoload": {
        "psr-4": { "Greenter\\Sunat\\ConsultaCpe\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Greenter\\Sunat\\ConsultaCpe\\" : "tests/" }
    },
    "suggest": {
        "ext-mbstring": "guzzle ^6, phpunit required"
    }
}
